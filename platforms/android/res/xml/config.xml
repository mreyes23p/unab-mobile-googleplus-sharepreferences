<?xml version='1.0' encoding='utf-8'?>
<widget id="com.unab.mobile.googleplus" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="GooglePlus">
        <param name="android-package" value="nl.xservices.plugins.GooglePlus" />
        <param name="onload" value="true" />
    </feature>
    <access origin="https://accounts.google.com/*" />
    <feature name="Sharedpreferences">
        <param name="android-package" value="in.edelworks.sharedpreferences.Sharedpreferences" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <name>Unab Mobile Example</name>
    <description>
        Ejemplo de uso de shared preferences para almacenar jsonwebtokens y uso de google plus
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
       Unab
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
